{
  "name": "autobyteus",
  "version": "1.0.0",
  "description": "AutoByteUs Desktop Application",
  "author": {
    "name": "AutoByteUs Team",
    "email": "team@autobyteus.com"
  },
  "main": "dist/electron/main.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ricardobalk/nuxt3-tailwindcss.git"
  },
  "scripts": {
    "dev": "nuxt dev --host 0.0.0.0",
    "build": "nuxt build",
    "test": "vitest",
    "start": "electron .",
    "generate": "nuxt generate",
    "generate:electron": "BUILD_TARGET=electron nuxt generate",
    "preview": "nuxt preview",
    "codegen": "graphql-codegen --require dotenv/config --config codegen.ts",
    "transpile-electron": "tsc -p electron/tsconfig.json",
    "build:electron": "yarn generate:electron && yarn transpile-electron && node build.js",
    "build:electron:linux": "yarn generate:electron && yarn transpile-electron && node build.js --linux",
    "build:electron:windows": "yarn generate:electron && yarn transpile-electron && node build.js --windows",
    "build:electron:mac": "yarn generate:electron && yarn transpile-electron && node build.js --mac"
  },
  "dependencies": {
    "@capacitor/android": "^6.1.2",
    "@capacitor/cli": "^3.9.0",
    "@capacitor/core": "^3.9.0",
    "@nuxt/schema": "^3.13.1",
    "@types/prismjs": "^1.26.4",
    "axios": "^1.7.7",
    "electron-is-dev": "^2.0.0",
    "pinia": "^2.1.7",
    "prismjs": "^1.29.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@graphql-codegen/typescript-vue-apollo": "^4.1.1",
    "@nuxt/test-utils": "^3.14.3",
    "@nuxtjs/apollo": "^5.0.0-alpha.14",
    "@pinia/nuxt": "^0.5.1",
    "@types/node": "^20.4.2",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.0.1",
    "dotenv": "^16.4.5",
    "electron": "^32.1.0",
    "electron-builder": "^25.0.5",
    "happy-dom": "^15.7.4",
    "jsdom": "^25.0.1",
    "nuxt": "^v3.12.3",
    "nuxt-electron": "^0.7.0",
    "playwright-core": "^1.48.0",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.5",
    "typescript": "^5.1.6",
    "vite": "^4.4.0",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vitest": "^2.1.2",
    "xmldom": "^0.6.0"
  }
}