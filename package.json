{
  "name": "autobyteus",
  "version": "1.0.0",
  "description": "AutoByteus Desktop Application",
  "license": "MIT",
  "author": {
    "name": "AutoByteus Team",
    "email": "team@autobyteus.com"
  },
  "main": "dist/electron/main.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ricardobalk/nuxt3-tailwindcss.git"
  },
  "scripts": {
    "dev": "nuxt dev --host 0.0.0.0",
    "build": "nuxt build",
    "test": "vitest",
    "start": "electron .",
    "generate": "nuxt generate",
    "generate:electron": "cross-env NODE_ENV=production BUILD_TARGET=electron nuxt generate",
    "generate:icons": "tsc -p build/tsconfig.json && node build/scripts/generateIcons.js",
    "preview": "nuxt preview",
    "codegen": "graphql-codegen --require dotenv/config --config codegen.ts",
    "transpile-electron": "tsc -p electron/tsconfig.json",
    "transpile-build": "tsc -p build/tsconfig.json",
    "prepare-server": "chmod +x ./scripts/prepare-server.sh && ./scripts/prepare-server.sh",
    "build:electron": "cross-env NODE_ENV=production yarn prepare-server && yarn generate:electron && yarn transpile-electron && tsc -p build/tsconfig.json && node build/dist/build.js",
    "build:electron:linux": "cross-env NODE_ENV=production yarn prepare-server && yarn generate:electron && yarn transpile-electron && tsc -p build/tsconfig.json && node build/dist/build.js --linux",
    "build:electron:windows": "cross-env NODE_ENV=production yarn prepare-server && yarn generate:electron && yarn transpile-electron && tsc -p build/tsconfig.json && node build/dist/build.js --windows",
    "build:electron:mac": "cross-env NODE_ENV=production yarn prepare-server && yarn generate:electron && yarn transpile-electron && tsc -p build/tsconfig.json && node build/dist/build.js --mac"
  },
  "dependencies": {
    "@capacitor/android": "^6.1.2",
    "@capacitor/cli": "^3.9.0",
    "@capacitor/core": "^3.9.0",
    "@heroicons/vue": "^2.2.0",
    "@nuxt/schema": "^3.13.1",
    "@types/prismjs": "^1.26.4",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "axios": "^1.7.7",
    "chart.js": "^4.4.7",
    "dompurify": "^3.2.2",
    "electron-is-dev": "^2.0.0",
    "html2canvas": "^1.4.1",
    "markdown-it": "^14.1.0",
    "markdown-it-prism": "^2.3.0",
    "monaco-editor": "^0.52.2",
    "pinia": "^2.1.7",
    "prismjs": "^1.29.0",
    "uuid": "^11.0.3",
    "vue-chartjs": "^5.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@graphql-codegen/typescript-vue-apollo": "^4.1.1",
    "@monaco-editor/loader": "^1.4.0",
    "@nuxt/test-utils": "^3.14.3",
    "@nuxtjs/apollo": "^5.0.0-alpha.14",
    "@pinia/nuxt": "^0.5.1",
    "@pinia/testing": "^0.1.7",
    "@tailwindcss/typography": "^0.5.15",
    "@types/electron-builder": "^5.0.1",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^20.4.2",
    "@types/to-ico": "^1.1.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "electron": "^32.1.0",
    "electron-builder": "^25.0.5",
    "happy-dom": "^15.7.4",
    "jsdom": "^25.0.1",
    "nuxt": "^v3.12.3",
    "nuxt-electron": "^0.7.0",
    "playwright-core": "^1.48.0",
    "postcss": "^8.4.39",
    "sharp": "^0.33.2",
    "tailwindcss": "^3.4.5",
    "to-ico": "^1.1.5",
    "typescript": "^5.1.6",
    "vite": "^4.4.0",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vitest": "^2.1.2",
    "xmldom": "^0.6.0"
  }
}
